<html>
  <head>
    <meta charset="utf-8">
    <title>GASNAP</title>
      <link href="index.scss" rel="stylesheet" type="text/css">
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>


<body>
    <header>

        <video autoplay loop class="video-background" muted plays-inline>
          <source src="https://zippy.gfycat.com/SeriousKeenHousefly.webm" type="video/webm"></video>

    <div class="nav">
    <span id="sign_up_pic"><%= image_tag("tranbkg_pic.png__360.png", :alt => "rss feed") %></span>

      <ul class="menu">
        <!-- <li><%= link_to "login", sign_in_path %></li> -->
        <!-- product_path -->
      </ul>
    </div>


      </header>

      <h1>Sign In</h1>

      <div class="form">

        <%= form_for(:user, :url => session_path(:user)) do |f| %>
        <div >
          <%= f.text_field :email %>
        </div>

        <div>
          <%= f.password_field :password %>
        </div>

        <%= f.check_box :remember_me %>

        <div>
          <%= f.label :remember_me %>
        </div>

          <button class="sign_in_in_sign_in"><%= link_to "Sign In", sign_in_path %></button>
          <br>

        <button class="sign_in_in_sign_in">
          <%= link_to "Forgot your password?", new_password_path(:user) %>
        </button>

        <p>Not a member yet?</p>
        <br>
        <button class="sign_in_in_sign_in"><%= link_to "Sign Up Today!",  sign_up_path %></button>
        <br>

        <div class="msg">
          <h1>GASNAP</h1>
          <h2>SNAP SNAP Here comes the gas!</h2>
        </div>
        <% end %>
  </body>
